// Example test file for monomorphization
fn identity<T>(value: T): T {
    value
}

fn main() {
    // These calls will cause monomorphization of the identity function with different types
    var _int_value = identity(42);
    var _str_value = identity("Hello, World!");
    var _bool_value = identity(true);

    // The compiler will generate three versions of the identity function:
    // 1. identity_int(int) -> int
    // 2. identity_str(char*) -> char*
    // 3. identity_bool(bool) -> bool
}
